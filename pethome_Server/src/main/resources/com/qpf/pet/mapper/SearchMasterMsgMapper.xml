<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qpf.pet.mapper.SearchMasterMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="searchMasterMsgResultMap" type="com.qpf.pet.pojo.SearchMasterMsg">
                <result column="title" property="title"/>
                <result column="state" property="state"/>
                <result column="name" property="name"/>
                <result column="age" property="age"/>
                <result column="gender" property="gender"/>
                <result column="coat_color" property="coatColor"/>
                <result column="resources" property="resources"/>
                <result column="pet_type" property="petType"/>
                <result column="price" property="price"/>
                <result column="address" property="address"/>
                <result column="user_id" property="userId"/>
                <result column="shop_id" property="shopId"/>
                <result column="note" property="note"/>
    </resultMap>

    <select id="findByPage" resultMap="searchMasterMsgResultMap">
        select * from t_search_master_msg
        <if test="ew != null and ew != ''">
            ${ew.customSqlSegment}
        </if>
        <!--&lt;!&ndash; 如果配置了MybatisPlus的分页插件,则该操作不需要&ndash;&gt;-->
        <!--<if test="page != null">-->
        <!--    limit ${(page.current - 1) * page.size}, #{page.size}-->
        <!--</if>-->
    </select>


</mapper>
